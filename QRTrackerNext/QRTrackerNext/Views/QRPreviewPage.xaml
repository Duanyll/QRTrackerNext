<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="QRTrackerNext.Views.QRPreviewPage"
             Title="二维码扫描预览">
    <ContentPage.Content>
        <StackLayout Padding="10">
            <Button x:Name="btnScanQR" Text="使用默认界面扫描二维码" Clicked="btnScanQR_Clicked"></Button>
            <Button x:Name="btnScanQRCustom" Text="使用自定义界面扫描二维码" Clicked="btnScanQRCustom_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>